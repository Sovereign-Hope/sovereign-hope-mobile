{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "lint": "eslint . --ext .js,.jsx,.tx,.tsx",
    "prepare": "husky install",
    "testChanged": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebugCurrent": "jest -o --watch --coverage=false",
    "test": "jest",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react-native": "4.2.2",
    "axios": "^0.24.0",
    "expo": "^46.0.0",
    "expo-apple-authentication": "~4.3.0",
    "expo-application": "~4.2.2",
    "expo-auth-session": "~3.7.1",
    "expo-constants": "~13.2.4",
    "expo-device": "~4.3.0",
    "expo-firebase-analytics": "~7.2.0",
    "expo-haptics": "~11.3.0",
    "expo-notifications": "~0.16.1",
    "expo-random": "~12.3.0",
    "expo-secure-store": "~11.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.6",
    "firebase": "^9.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-render-html": "^6.3.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-section-list-get-item-layout": "^2.2.3",
    "react-redux": "^7.2.4",
    "sentry-expo": "~5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-typescript": "7.14.5",
    "@expo/config": "^7.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "26.0.23",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "@types/react-redux": "^7.1.18",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.29.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native-a11y": "2.0.4",
    "eslint-plugin-unicorn": "33.0.1",
    "expo-build-properties": "^0.3.0",
    "husky": "6.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "react-native-accessibility-engine": "^0.3.3",
    "react-test-renderer": "17.0.2",
    "redux-devtools": "^3.7.0",
    "typescript": "^4.6.3"
  },
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/.eslintrc.js"
    ]
  }
}
