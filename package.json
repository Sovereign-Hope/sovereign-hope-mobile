{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint . --ext .js,.jsx,.tx,.tsx",
    "prepare": "husky install",
    "testChanged": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebugCurrent": "jest -o --watch --coverage=false",
    "test": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/config-plugins": "~8.0.0",
    "@expo/metro-config": "~0.18.11",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "11.3.1",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-menu/menu": "^0.7.2",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react-native": "~5.22.0",
    "@types/react-native-rss-parser": "^1.4.0",
    "axios": "^1.7.7",
    "expo": "~51.0.31",
    "expo-apple-authentication": "~6.4.2",
    "expo-application": "~5.9.1",
    "expo-auth-session": "~5.5.2",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-device": "~6.0.2",
    "expo-haptics": "~13.0.1",
    "expo-notifications": "~0.28.16",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.24",
    "firebase": "^9.23.0",
    "node-html-parser": "^6.1.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-render-html": "^6.3.0",
    "react-native-rss-parser": "^1.5.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-section-list-get-item-layout": "^2.2.3",
    "react-native-track-player": "^3.2.0",
    "react-redux": "^7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-typescript": "7.14.5",
    "@expo/config": "~9.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "26.0.24",
    "@types/react": "~18.2.45",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.27.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native-a11y": "3.3.0",
    "eslint-plugin-unicorn": "44.0.2",
    "expo-build-properties": "~0.12.5",
    "husky": "6.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "lint-staged": "11.0.0",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.3.1",
    "react-native-accessibility-engine": "^0.3.3",
    "react-test-renderer": "18.0.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|native-base|@sentry/.*)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/.eslintrc.js"
    ]
  },
  "name": "sovereign-hope-mobile",
  "version": "1.0.0"
}
