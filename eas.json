{
  "build": {
    "production": {
      "ios": {
        "autoIncrement": true
      },
      "releaseChannel": "prod-v1"
    },
    "staging": {
      "releaseChannel": "rc-v1"
    },
    "development": {
      "releaseChannel": "development",
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "distribution": "internal"
      },
      "ios": {
        "simulator": true
      },
      "releaseChannel": "development"
    }
  },
  "cli": {
    "version": ">= 0.35.0"
  }
}
